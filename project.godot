; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="Bodega"
run/main_scene="res://Main.tscn"
config/icon="res://icon.png"

[autoload]

Transition="*res://Singleton/Transition.tscn"
SceneCache="*res://Singleton/SceneCache.tscn"

[display]

window/size/width=800
window/size/resizable=false
window/stretch/mode="viewport"
window/stretch/aspect="keep"
window/stretch/shrink="1"

[input]

drop=[ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":68,"unicode":0,"echo":false,"script":null)
 ]

[layer_names]

2d_physics/layer_1="Player"
2d_physics/layer_2="Enemy"
2d_physics/layer_3="Friend"
2d_physics/layer_4="Bullet"
2d_physics/layer_5="Map"

[rendering]

environment/default_environment="res://default_env.tres"
